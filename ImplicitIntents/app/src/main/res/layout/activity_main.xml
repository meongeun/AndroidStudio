<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="mOnClick"
        android:text="내부 액티비티 실행 (1)"
        android:id="@+id/btnTest1"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="mOnClick"
        android:id="@+id/btnTest2"
        android:text="내부 액티비티 실행 (2)"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btnTest3"
        android:onClick="mOnClick"
        android:text="메인 액티비티 실행"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btnTest4"
        android:onClick="mOnClick"
        android:text="전화걸기 액티비티 실행"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btnTest5"
        android:onClick="mOnClick"
        android:text="웹보기 액티비티 실행"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/btnTest6"
        android:onClick="mOnClick"
        android:text="홈화면 액티비티 실행"/>

</LinearLayout>